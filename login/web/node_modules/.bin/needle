#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/jaxztan/Documents/hack/devmatch/res/web/node_modules/needle/bin/node_modules:/Users/jaxztan/Documents/hack/devmatch/res/web/node_modules/needle/node_modules:/Users/jaxztan/Documents/hack/devmatch/res/web/node_modules:/Users/jaxztan/Documents/hack/devmatch/res/node_modules:/Users/jaxztan/Documents/hack/devmatch/node_modules:/Users/jaxztan/Documents/hack/node_modules:/Users/jaxztan/Documents/node_modules:/Users/jaxztan/node_modules:/Users/node_modules:/node_modules:/Users/jaxztan/Documents/hack/devmatch/res/web/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/jaxztan/Documents/hack/devmatch/res/web/node_modules/needle/bin/node_modules:/Users/jaxztan/Documents/hack/devmatch/res/web/node_modules/needle/node_modules:/Users/jaxztan/Documents/hack/devmatch/res/web/node_modules:/Users/jaxztan/Documents/hack/devmatch/res/node_modules:/Users/jaxztan/Documents/hack/devmatch/node_modules:/Users/jaxztan/Documents/hack/node_modules:/Users/jaxztan/Documents/node_modules:/Users/jaxztan/node_modules:/Users/node_modules:/node_modules:/Users/jaxztan/Documents/hack/devmatch/res/web/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../needle/bin/needle" "$@"
else
  exec node  "$basedir/../needle/bin/needle" "$@"
fi
